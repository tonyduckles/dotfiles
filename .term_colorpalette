# Use Linux "set palette" escape sequence to customize ANSI colors in mintty
# http://groups.google.com/group/mintty-discuss/browse_thread/thread/33cb565e4f32f146

# (Solarized-inspired)
echo -n -e "\e]P0020303"  # black
echo -n -e "\e]P1990000"  # red
echo -n -e "\e]P24B8700"  # green
echo -n -e "\e]P3A0A533"  # yellow
echo -n -e "\e]P40A4BFF"  # blue
echo -n -e "\e]P5980094"  # magenta
echo -n -e "\e]P6009A91"  # cyan
echo -n -e "\e]P78AA0A8"  # white
echo -n -e "\e]P84C4D5D"  # brblack
echo -n -e "\e]P9DC322F"  # brred
echo -n -e "\e]PA72D631"  # brgreen
echo -n -e "\e]PBFEFF00"  # bryellow
echo -n -e "\e]PC268BD2"  # brblue
echo -n -e "\e]PDE900E3"  # brmagenta
echo -n -e "\e]PE00D8CC"  # brcyan
echo -e "\e]PFF8F6F4"     # brwhite

